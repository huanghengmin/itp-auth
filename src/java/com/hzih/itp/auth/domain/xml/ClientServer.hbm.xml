<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class table="client_server" name="com.hzih.itp.auth.domain.ClientServer">
        <id access="field" name="id" type="int" column="id" length="11">
            <generator class="increment"/>
        </id>
        <property name="clientId" length="11" column="clientId" access="field" type="int" />
        <property name="serverId" length="11" column="serverId" access="field" type="int"/>
        <property name="appName" length="30" column="appName" access="field" type="string"/>
        <property name="path" length="255" column="path" access="field" type="string"/>
    </class>
</hibernate-mapping>
